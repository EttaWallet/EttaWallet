    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.76.5",
    "pod-install": "^0.1.0",
    "query-string": "^8.1.0",
    "react-native-flipper": "^0.207.0",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.1.0",
